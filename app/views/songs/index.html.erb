<h1>Song Index</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Genre</th>
      <th>Year</th>
    </tr>
  </thead>
  <tbody>    
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= song.release_year %></td>
        <td>
          <%= link_to "View", song_path(song) %>
          | <%= link_to "Edit", edit_song_path(song) %>
          | <%= link_to "Delete", song_path(song), method: :delete %>
        </td>
      </tr>   
    <% end %>
  </tbody>
</table>

<br> <br>
<%= link_to 'Create a Song', new_song_path %>